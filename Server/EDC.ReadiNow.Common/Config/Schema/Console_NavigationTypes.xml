  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--    CONSOLE BEHAVIORS                                             -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!-- 'Console Behavior' type -->
  <type>
    <alias>k:consoleBehavior</alias>
    <name>Console Behavior</name>
    <description>Describes how a resource should appear and behave in the console.</description>
    <generateCode>true</generateCode>
    <isMetadata>true</isMetadata>
    <fieldGroups>
      <fieldGroup>
        <name>Console Behavior Details</name>
        <alias>k:consoleBehaviorDetails</alias>
      </fieldGroup>
    </fieldGroups>
    <fields>
      <stringField>
        <name>Tree icon URL</name>
        <alias>k:treeIconUrl</alias>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
      </stringField>
      <stringField>
        <name>Tree icon background color (ARGB)</name>
        <alias>k:treeIconBackgroundColor</alias>
        <fieldRepresents>fieldRepresentsColor</fieldRepresents>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
      </stringField>
      <stringField>
        <name>HTML5 view identifier</name>
        <alias>k:html5ViewId</alias>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
      </stringField>
      <stringField>
        <name>HTML5 create identifier</name>
        <description>HTML state to use when creating new instances of this type.</description>
        <alias>k:html5CreateId</alias>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
      </stringField>
      <boolField>
        <name>Suppress actions for type</name>
        <alias>k:suppressActionsForType</alias>
        <description>If specified on the console behavior of an individual resource, will prevent additional action menus being loaded that apply to the type.</description>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
      </boolField>
      <boolField>
        <name>Console behavior hidden</name>
        <alias>k:consoleBehaviorHidden</alias>
        <description>Hides the console behavior.</description>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
        <hideField>true</hideField>
        <hideFieldDefaultForm>true</hideFieldDefaultForm>
        <defaultValue>false</defaultValue>
      </boolField>
    </fields>
    <relationships>
      <!-- relationship to an Icon entity. Independent of 'k:treeIconUrl' string field above.  -->
      <relationship>
        <alias>k:treeIcon</alias>
        <name>Tree icon</name>
        <description>The icon entity for the console behavior.</description>
        <toType>iconFileType</toType>
        <relType>relCustom</relType>
        <cardinality>manyToOne</cardinality>
        <generateCode>true</generateCode>
        <cloneAction>cloneReferences</cloneAction>
        <reverseCloneAction>drop</reverseCloneAction>
        <cascadeDelete>false</cascadeDelete>
        <cascadeDeleteTo>false</cascadeDeleteTo>
        <implicitInSolution>true</implicitInSolution>
        <reverseImplicitInSolution>false</reverseImplicitInSolution>
        <relationshipInFromTypeGroup>k:consoleBehaviorDetails</relationshipInFromTypeGroup>
        <relationshipInToTypeGroup>iconFileTypeDetails</relationshipInToTypeGroup>
        <securesTo>true</securesTo>
      </relationship>
    </relationships>
  </type>
  
  <!-- 'Type Console Behavior' relationship -->
  <relationship>
    <alias>k:typeConsoleBehavior</alias>
    <reverseAlias>k:behaviorForTypes</reverseAlias>
    <name>Type console behavior</name>
    <description>Links a resource type to the way instances of that type should appear and behave in the console.</description>
    <fromType>type</fromType>
    <toType>k:consoleBehavior</toType>
    <relType>relSingleComponent</relType>
    <securesTo>true</securesTo>
    <hideOnFromType>true</hideOnFromType>
    <generateCode>true</generateCode>
    <relationshipInToTypeGroup>k:consoleBehaviorDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- 'Resource Console Behavior' relationship -->
  <relationship>
    <alias>k:resourceConsoleBehavior</alias>
    <reverseAlias>k:behaviorForResources</reverseAlias>
    <name>Resource console behavior</name>
    <description>Links an individual resource to a description of how it should behave in the console.</description>
    <fromType>resource</fromType>
    <toType>k:consoleBehavior</toType>
    <relType>relSingleComponent</relType>
    <securesTo>true</securesTo>
    <generateCode>true</generateCode>
    <hideOnFromType>true</hideOnFromType>
    <relationshipInToTypeGroup>k:consoleBehaviorDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- 'Selection Behavior' relationship -->
  <relationship>
    <alias>k:selectionBehavior</alias>
    <reverseAlias>k:behaviorForSelection</reverseAlias>
    <name>Selection behavior</name>
    <description>Links a report, chart, or relationship control to the way that its selected items should show their context menu.</description>
    <fromType>resource</fromType>
    <toType>k:consoleBehavior</toType>
    <securesTo>true</securesTo>
    <relType>relSingleComponent</relType>
    <generateCode>true</generateCode>
    <hideOnFromType>true</hideOnFromType>
    <relationshipInToTypeGroup>k:consoleBehaviorDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- 'Default Display Report' relationship -->
  <relationship>
    <alias>defaultDisplayReport</alias>
    <reverseAlias>isDefaultDisplayReportForTypes</reverseAlias>
    <name>Default display report</name>
    <description>Associates default report to use when displaying instances of a given type.</description>
    <fromType>type</fromType>
    <toType>report</toType>
    <securesTo>true</securesTo>
    <relType>relLookup</relType>
    <generateCode>true</generateCode>
    <relationshipInFromTypeGroup>typeDetails</relationshipInFromTypeGroup>
    <relationshipInToTypeGroup>reportDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- 'Default Picker Report' relationship -->
  <relationship>
    <alias>defaultPickerReport</alias>
    <reverseAlias>isDefaultPickerReportForTypes</reverseAlias>
    <name>Default picker report</name>
    <description>Associates default report to use when picking instances of a given type.</description>
    <fromType>type</fromType>
    <toType>resourcePicker</toType>
    <securesTo>true</securesTo>
    <relType>relLookup</relType>
    <generateCode>true</generateCode>
    <relationshipInFromTypeGroup>typeDetails</relationshipInFromTypeGroup>
    <relationshipInToTypeGroup>reportDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--    ACTIONS MENU ITEMS                                            -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <type>
    <alias>k:actionExpression</alias>
    <name>Action Expression</name>
    <description>An optional expression that determines the applicability of an action menu item.</description>
    <generateCode>true</generateCode>
    <fieldGroups>
      <fieldGroup>
        <name>Action Expression Details</name>
        <alias>k:actionExpressionDetails</alias>
      </fieldGroup>
    </fieldGroups>
    <fields>
      <stringField>
        <alias>k:actionExpressionString</alias>
        <name>Expression</name>
        <description>The expression to evaluate.</description>
        <fieldInGroup>k:actionExpressionDetails</fieldInGroup>
      </stringField>
    </fields>
    <relationships>
      <relationship>
        <alias>k:actionExpressionEntities</alias>
        <name>Known entities</name>
        <description>Entities that may be used in the expression.</description>
        <toType>namedReference</toType>
        <relType>relComponents</relType>
        <generateCode>true</generateCode>
        <relationshipInFromTypeGroup>k:actionExpressionDetails</relationshipInFromTypeGroup>
        <relationshipInToTypeGroup>k:actionExpressionDetails</relationshipInToTypeGroup>
        <securesTo>true</securesTo>
      </relationship>
    </relationships>
  </type>
  <!-- 'Action Menu Item' type -->
  <type>
    <alias>k:actionMenuItem</alias>
    <name>Action Menu Item</name>
    <description>Represents an action menu item.</description>
    <generateCode>true</generateCode>
    <isMetadata>true</isMetadata>
    <fieldGroups>
      <fieldGroup>
        <name>Action Menu Item Details</name>
        <alias>k:actionMenuItemDetails</alias>
      </fieldGroup>
    </fieldGroups>
    <fields>
      <stringField>
        <name>Multi select name</name>
        <alias>multiSelectName</alias>
        <description>The label to present if the action is applied to a multi-selection.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
      <stringField>
        <name>Empty select name</name>
        <alias>emptySelectName</alias>
        <description>The label to present if the action is disabled or when selection information is to be excluded.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
      <stringField>
        <name>Menu icon URL</name>
        <alias>k:menuIconUrl</alias>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
      <intField>
        <name>Menu order</name>
        <alias>k:menuOrder</alias>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </intField>
      <boolField>
        <name>Is separator</name>
        <alias>k:isMenuSeparator</alias>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <boolField>
        <name>Is action button</name>
        <alias>k:isActionButton</alias>
        <description>This Action item will appear as a button.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <boolField>
        <name>Is double-click action</name>
        <alias>k:isDoubleClickAction</alias>
        <description>This action is executed if an item is double-clicked.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <boolField>
        <!-- Siverlight specific hack .. ignore -->
        <name>Is stacked</name>
        <alias>k:isStackedLandingPage</alias>
        <description>This action is a landing page that appears over the top of an existing one.</description>
        <defaultValue>false</defaultValue>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <stringField>
        <!-- Data that gets passed to the action class (feel free to use, or not) -->
        <name>Control action verb</name>
        <alias>k:controlAction</alias>
        <description>The action verb passed to the control.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
      <stringField>
        <!-- More data that gets passed to the action class (feel free to use, or not) -->
        <name>Control action data</name>
        <alias>k:controlActionData</alias>
        <description>Additional data passed to the control action.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
      <boolField>
        <!-- Does this action depend on some user selection state -->
        <name>Applies to selection</name>
        <alias>k:appliesToSelection</alias>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <boolField>
        <!-- Does this action depend on more than one item present in the user selection state -->
        <name>Applies to multi selection</name>
        <alias>k:appliesToMultiSelection</alias>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <boolField>
        <!-- When looking at Console_Behavior.xml, ignore anything that has this set to true -->
        <name>Is an action item</name>
        <description>Is visible in the legacy action menu system.</description>
        <alias>k:isActionItem</alias>
        <defaultValue>false</defaultValue>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <boolField>
        <!-- When looking at Console_Behavior.xml, ignore anything that does not have this set to true -->
        <name>Is a context menu</name>
        <description>Is visible in context menus.</description>
        <alias>k:isContextMenu</alias>
        <defaultValue>false</defaultValue>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <boolField>
        <!-- Denotes that the action is considered as "out of the box" and has special privilege -->
        <name>Is a system item</name>
        <alias>k:isSystem</alias>
        <description>Denotes this action item is owned by the system.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </boolField>
      <stringField>
        <!-- Identifies the core action that is happening behind the item -->
        <name>HTML action</name>
        <alias>htmlActionMethod</alias>
        <description>The action verb passed to the service.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
      <stringField>
        <!-- Further specifies a sub-category of task to be applied to the action method -->
        <name>HTML action state</name>
        <alias>htmlActionState</alias>
        <description>Additional data passed to the html action.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
      <stringField>
        <!-- Optionally point the action at a related entity, other than the default -->
        <name>HTML action target</name>
        <alias>htmlActionTarget</alias>
        <description>Specifies an alternate target to the action then that inherited from the behavior.</description>
        <fieldInGroup>k:actionMenuItemDetails</fieldInGroup>
      </stringField>
    </fields>
  </type>
  <!-- 'Action Menu Item to Role' relationship -->
  <relationship>
    <alias>actionRequiresRole</alias>
    <reverseAlias>roleFromAction</reverseAlias>
    <name>Action menu item requires role</name>
    <description>Associates an action menu item to required role on the target entity(ies).</description>
    <fromName>Action menu items</fromName>
    <fromType>k:actionMenuItem</fromType>
    <toName>Requires role</toName>
    <toType>role</toType>
    <relType>relLookup</relType>
    <generateCode>true</generateCode>
    <relationshipInFromTypeGroup>k:actionMenuItemDetails</relationshipInFromTypeGroup>
    <relationshipInToTypeGroup>roleDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- 'Action Menu Item to Permission' relationship -->
  <relationship>
    <alias>k:actionRequiresPermission</alias>
    <reverseAlias>k:permissionFromAction</reverseAlias>
    <name>Action menu item requires permission</name>
    <description>Associates an action menu item to required permission(s) on the target entity(ies).</description>
    <fromName>Action menu items</fromName>
    <fromType>k:actionMenuItem</fromType>
    <toName>Requires permission</toName>
    <toType>permission</toType>
    <relType>relManyToMany</relType>
    <generateCode>true</generateCode>
    <relationshipInFromTypeGroup>k:actionMenuItemDetails</relationshipInFromTypeGroup>
    <relationshipInToTypeGroup>permissionDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- 'Action Menu Item to Expression' relationship -->
  <relationship>
    <alias>k:actionRequiresExpression</alias>
    <reverseAlias>k:expressionFromAction</reverseAlias>
    <name>Action menu item requires expression</name>
    <description>Associates an action menu item to an expression that determines its applicability.</description>
    <fromName>Action menu item</fromName>
    <fromType>k:actionMenuItem</fromType>
    <toName>Expression</toName>
    <toType>k:actionExpression</toType>
    <relType>relSingleComponent</relType>
    <generateCode>true</generateCode>
    <relationshipInFromTypeGroup>k:actionMenuItemDetails</relationshipInFromTypeGroup>
    <relationshipInToTypeGroup>k:actionExpressionDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- MultiRelationshipControlBehavior type -->
  <type>
    <name>MultiRelationshipControlBehavior</name>
    <description>Describes the behavior of buttons in the relationship tab.</description>
    <alias>k:multiRelationshipControlBehavior</alias>
    <generateCode>true</generateCode>
    <isMetadata>true</isMetadata>
    <fieldGroups>
      <fieldGroup>
        <name>MultiRelationshipControlBehavior Details</name>
        <alias>k:multiRelationshipControlBehaviorDetails</alias>
      </fieldGroup>
    </fieldGroups>
  </type>
  <!-- 'Action Menu Item' to 'MultiRelationshipControlBehavior' relationship -->
  <relationship>
