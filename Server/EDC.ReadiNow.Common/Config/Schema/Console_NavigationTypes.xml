  <!-- Console solution resource -->
  <solution>
    <alias>consoleSolution</alias>
    <name>ReadiNow Console</name>
    <description>System application that contains user interface objects.</description>
    <solutionVersionString>1.0.0.0</solutionVersionString>
    <solutionPublisher>Readinow Corporation</solutionPublisher>
    <solutionPublisherUrl>http://www.readinow.com</solutionPublisherUrl>
    <solutionReleaseDate>2012-12-13</solutionReleaseDate>
    <legacyGuid>1B5EEEFB-E129-439A-B814-764B0D384BC4</legacyGuid>
    <packageId>8aeb3b0b-21ce-49f3-96ca-90103e62eea6</packageId>
  </solution>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--    TREE NAVIGATION ELEMENTS                                      -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!-- 'Navigation Element' type -->
  <type>
    <alias>k:navigationElement</alias>
    <name>Navigation Element</name>
    <description>Provides properties common to many console navigation resources.</description>
    <generateCode>true</generateCode>
    <isMetadata>true</isMetadata>
    <fieldGroups>
      <fieldGroup>
        <name>Navigation Element Details</name>
        <alias>k:navigationElementDetails</alias>
      </fieldGroup>
    </fieldGroups>
    <fields>
      <intField>
        <alias>k:consoleOrder</alias>
        <name>Order</name>
        <description>A number to specify the sorting order of this item.</description>
        <fieldInGroup>k:navigationElementDetails</fieldInGroup>
      </intField>
      <stringField>
        <name>Tree icon background color (ARGB)</name>
        <alias>k:navElementTreeIconBackgroundColor</alias>
        <fieldRepresents>fieldRepresentsColor</fieldRepresents>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
      </stringField>
      <stringField>
        <name>Feature switch</name>
        <description>If set to a feature switch name, that switch must be enabled for the nav element to be visible.</description>
        <alias>navFeatureSwitch</alias>
        <hideField>true</hideField>
        <fieldInGroup>k:consoleBehaviorDetails</fieldInGroup>
      </stringField>
    </fields>
    <derivedTypes>report,chart,board</derivedTypes>
  </type>
  <!-- 'Navigation Element' type -->
  <type>
    <alias>k:navContainer</alias>
    <name>Navigation Container</name>
    <description>Base type for any definition that can act as a console 'folder'.</description>
    <generateCode>true</generateCode>
    <inherits>k:navigationElement,visualController</inherits>
    <derivedTypes>documentFolder</derivedTypes>
    <isMetadata>true</isMetadata>
    <fieldGroups>
      <fieldGroup>
        <name>Navigation Container Details</name>
        <alias>k:navContainerDetails</alias>
      </fieldGroup>
    </fieldGroups>
    <!-- late declaration of inheritance -->
  </type>
  <!-- 'Static Page' type -->
  <type>
    <alias>k:staticPage</alias>
    <name>Console Page</name>
    <description>Represents a non-dynamic console page.</description>
    <inherits>k:navigationElement</inherits>
    <generateCode>true</generateCode>
  </type>
  <!-- 'Top Menu' type -->
  <type>
    <alias>k:topMenu</alias>
    <name>Top Menu</name>
    <description>Represents a menu item at the top of the console.</description>
    <inherits>k:navContainer</inherits>
    <generateCode>true</generateCode>
    <isMetadata>true</isMetadata>
    <fieldGroups>
      <fieldGroup>
        <name>Top Menu Details</name>
        <alias>k:topMenuDetails</alias>
      </fieldGroup>
    </fieldGroups>
    <fields>
      <boolField>
        <name>Is visible</name>
        <alias>k:isTopMenuVisible</alias>
        <defaultValue>true</defaultValue>
        <fieldInGroup>k:topMenuDetails</fieldInGroup>
      </boolField>
      <boolField>
        <name>Show application tabs</name>
        <alias>k:showApplicationTabs</alias>
        <defaultValue>false</defaultValue>
        <fieldInGroup>k:topMenuDetails</fieldInGroup>
      </boolField>
    </fields>
  </type>
  <!-- 'Navigation Section' type -->
  <type>
    <alias>k:navSection</alias>
    <name>Navigation Section</name>
    <description>Represents a section in the console left-hand navigation.</description>
    <inherits>k:navContainer</inherits>
    <generateCode>true</generateCode>
    <isMetadata>true</isMetadata>
    <fieldGroups>
      <fieldGroup>
        <name>Navigation Section Details</name>
        <alias>k:navSectionDetails</alias>
      </fieldGroup>
    </fieldGroups>
    <fields>
      <boolField>
        <name>Is application tab</name>
        <alias>k:isAppTab</alias>
        <defaultValue>false</defaultValue>
        <fieldInGroup>k:navSectionDetails</fieldInGroup>
      </boolField>
    </fields>
  </type>
  <!-- 'Folder' type -->
  <type>
    <name>Folder</name>
    <alias>folder</alias>
    <description>The 'Folder' type</description>
    <inherits>k:navContainer</inherits>
    <generateCode>true</generateCode>
  </type>
  <!-- 'Private content section' type -->
  <type>
    <name>Private Content Section</name>
    <alias>k:privateContentSection</alias>
    <description>The 'Private Section' type</description>
    <inherits>k:navContainer</inherits>
    <generateCode>true</generateCode>
  </type>
  <!-- Report in Security Folder (used to avoid resource key issues) -->
  <relationship>
    <name>Security report in folder</name>
    <description>Link a report used in an access rule to ensure cascading deletes.</description>
    <alias>securityReportInFolder</alias>
    <reverseAlias>folderForSecurityReport</reverseAlias>
    <fromType>report</fromType>
    <fromName>Report</fromName>
    <toType>folder</toType>
    <toName>Folder</toName>
    <generateCode>true</generateCode>
    <relType>relSingleComponent</relType>
    <relationshipInFromTypeGroup>reportDetails</relationshipInFromTypeGroup>
    <relationshipInToTypeGroup>reportDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- Allow Display -->
  <relationship>
    <name>Allowed display</name>
    <description>List of navigation elements that are allowed to be displayed.</description>
    <alias>allowDisplay</alias>
    <reverseAlias>allowedDisplayBy</reverseAlias>
    <fromType>subject</fromType>
    <fromName>Visible to subjects</fromName>
    <toType>k:navigationElement</toType>
    <toName>Allowed resources</toName>
    <generateCode>false</generateCode>
    <relType>relManyToMany</relType>
    <relationshipInFromTypeGroup>subjectDetails</relationshipInFromTypeGroup>
    <relationshipInToTypeGroup>k:navigationElementDetails</relationshipInToTypeGroup>
    <hideOnFromType>true</hideOnFromType>
    <hideOnToType>true</hideOnToType>
  </relationship>
  <!-- 'Resource in folder' relationship -->
  <relationship>
    <alias>k:resourceInFolder</alias>
    <name>Resource in folder</name>
    <description>This relationship indicates that a resource should appear in the console under the specified folder.</description>
    <reverseAlias>k:folderContents</reverseAlias>
    <fromType>resource</fromType>
    <toType>k:navContainer</toType>
    <cardinality>manyToMany</cardinality>
    <generateCode>true</generateCode>
    <hideOnFromType>true</hideOnFromType>
    <cascadeDelete>true</cascadeDelete>
    <cascadeDeleteTo>false</cascadeDeleteTo>
    <implicitInSolution>false</implicitInSolution>
    <reverseImplicitInSolution>true</reverseImplicitInSolution>
    <cloneAction>cloneReferences</cloneAction>
    <reverseCloneAction>drop</reverseCloneAction>
    <relType>relCustom</relType>
    <!-- TODO: This needs a careful review -->
    <relationshipInToTypeGroup>k:navContainerDetails</relationshipInToTypeGroup>
  </relationship>
  <!-- 'Resource shortcut in folder' relationship -->
  <relationship>
    <alias>k:shortcutInFolder</alias>
    <name>Shortcut in folder</name>
    <description>This relationship indicates that a resource should appear in the console under the specified folder as a shortcut.</description>
    <reverseAlias>k:shortcuts</reverseAlias>
    <fromType>resource</fromType>
    <toType>k:navContainer</toType>
    <relType>relManyToMany</relType>
    <generateCode>true</generateCode>
    <hideOnFromType>true</hideOnFromType>
    <relationshipInToTypeGroup>k:navContainerDetails</relationshipInToTypeGroup>
  </relationship>
