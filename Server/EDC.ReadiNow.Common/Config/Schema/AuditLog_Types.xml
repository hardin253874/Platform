        <toName>Minimum severity</toName>
        <relationshipInFromTypeGroup>auditLogSettingsDetails</relationshipInFromTypeGroup>
        <relationshipIsMandatory>true</relationshipIsMandatory>
      </relationship>
    </relationships>
  </type>
