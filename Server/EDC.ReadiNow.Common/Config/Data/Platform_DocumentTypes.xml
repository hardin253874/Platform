  <!-- Word Document Type -->
  <imageFileType source="../Binaries/WordDocumentImage.jpg">
    <alias>wordDocumentImage</alias>
    <name>Word Document</name>
    <description>Word document type.</description>
    <imageHeight>160</imageHeight>
    <imageWidth>160</imageWidth>
  </imageFileType>
  <documentType>
    <alias>wordDocumentDocumentType</alias>
    <name>Word Document</name>
    <shortFileTypeName>Word</shortFileTypeName>
    <fileTypeDescription>Word Files (*.doc;*.docx;*.docm;*.dotx;*.dot;*.dotm;*.rtf;*.odt)</fileTypeDescription>
    <extension>*.doc;*.docx;*.docm;*.dotx;*.dot;*.dotm;*.rtf;*.odt</extension>
    <mimeType>application/ms-word</mimeType>
    <documentTypeImage>wordDocumentImage</documentTypeImage>
  </documentType>
