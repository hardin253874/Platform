  <!-- The navigation configuration button with full admin access -->
  <k:navigationConfigButton>
    <alias>fullConfigButton</alias>
    <name>Full Configuration Button</name>
  </k:navigationConfigButton>
  <!-- The configuration button instance with self-service level access (reports, charts, screens) -->
  <k:navigationConfigButton>
    <alias>selfServiceConfigButton</alias>
    <name>Self-Service Configuration Button</name>
  </k:navigationConfigButton>
  <!-- The admin toolbox static page instance to host quick links -->
  <k:staticPage>
    <alias>k:adminToolboxStaticPage</alias>
    <name>Admin Toolbox</name>
  </k:staticPage>
  <!-- Tenant rollback page -->
  <k:staticPage>
    <alias>k:tenantRollbackStaticPage</alias>
    <name>Tenant Rollback</name>
    <k:consoleOrder>100</k:consoleOrder>
  </k:staticPage>
  <!-- Application dependencies -->
  <applicationDependency>
    <name>Console Solution Depends on Core Solution</name>
    <description>Console solution depends on Core solution being installed.</description>
    <alias>consoleToCoreDependency</alias>
    <applicationMinimumVersion>Any</applicationMinimumVersion>
    <applicationMaximumVersion>Any</applicationMaximumVersion>
    <applicationIsRequired>true</applicationIsRequired>
    <dependentApplication>consoleSolution</dependentApplication>
    <dependencyApplication>coreSolution</dependencyApplication>
  </applicationDependency>
</resources>