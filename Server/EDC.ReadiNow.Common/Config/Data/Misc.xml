    <k:consoleGeneralContentAreaContainerHeadingFontColor>
    </k:consoleGeneralContentAreaContainerHeadingFontColor>
    <k:consoleGeneralContentAreaContainerHeadingLineColor>
    </k:consoleGeneralContentAreaContainerHeadingLineColor>
    <k:consoleGeneralContentAreaSelectedTabColor>
    </k:consoleGeneralContentAreaSelectedTabColor>
    <k:consoleGeneralContentAreaSelectedTabFontColor>
