    </k:consoleHeaderBackgroundImage>
    <k:consoleHeaderBackgroundImageRepeat>true</k:consoleHeaderBackgroundImageRepeat>
    <k:consoleTopBackgroundImage>
    </k:consoleTopBackgroundImage>
    <k:consoleTopBackgroundImageRepeat>true</k:consoleTopBackgroundImageRepeat>
    <k:consoleLeftBackgroundImage>
    </k:consoleLeftBackgroundImage>
    <k:consoleLeftBackgroundImageRepeat>true</k:consoleLeftBackgroundImageRepeat>
    <k:consoleTopNavigationStyle>console:topNavigationStyle1</k:consoleTopNavigationStyle>
    <k:consoleHeaderBackgroundColor>
    </k:consoleHeaderBackgroundColor>
    <k:consoleHeaderMenuTextColor>
    </k:consoleHeaderMenuTextColor>
