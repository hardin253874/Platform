<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="core" xmlns:k="console" xsi:schemaLocation=". ../Core.xsd k ../Console.xsd" defaultSolution="consoleSolution">
  <!--Copyright 2011-2016 Global Software Innovation Pty Ltd-->
  
  
  <!-- Tenant Image settings -->
  <tenantImageSettings>
    <alias>tenantImageSettingsInstance</alias>
    <name>Image Settings</name>
    <description>The settings used to control images configured by the tenant.</description>
  </tenantImageSettings>
  <!-- Event log settings instance -->
  <eventLogSettings>
    <alias>tenantEventLogSettingsInstance</alias>
    <name>Event Log Settings</name>
    <description>The event log settings configured by the tenant.</description>
    <maxEventLogEntries>10000</maxEventLogEntries>
  </eventLogSettings>
  <!-- Tenant General settings instance -->
  <tenantGeneralSettings>
    <alias>tenantGeneralSettingsInstance</alias>
    <name>General Settings</name>
    <description>The general settings configured by the tenant.</description>
    <finYearStartMonth>moyJanuary</finYearStartMonth>
    <tenantCurrencySymbol>$</tenantCurrencySymbol>
    <k:tenantConsoleThemeSettings>k:tenantConsoleTheme</k:tenantConsoleThemeSettings>
  </tenantGeneralSettings>
  <!-- Notification provider-->
  <twilioNotifier>
    <alias>tenantNotificationProvider</alias>
    <name>Notification Provider</name>
    <description>The Twilio account used for sending notifications.</description>
    <tpAccountSid>Enter SID</tpAccountSid>
    <tpAuthToken>Empty value</tpAuthToken>
    <tpSendingNumber>12345-12345</tpSendingNumber>
    <tpEnableTestMode>true</tpEnableTestMode>
  </twilioNotifier>
  <!-- The software platform console image-->
  <photoFileType source="../Binaries/logo_softwareplatform.png">
    <alias>k:defaultProductLogoImage</alias>
    <name>Default Logo Image</name>
    <description>Default Logo Image.</description>
    <imageWidth>173</imageWidth>
    <imageHeight>72</imageHeight>
    <fileExtension>.png</fileExtension>
  </photoFileType>
  <iconFileType source="../Binaries/TopMenuImage_Home.png">
    <alias>k:topMenuImage_Home</alias>
    <name>Home Image</name>
    <description>
    </description>
    <imageWidth>200</imageWidth>
    <imageHeight>200</imageHeight>
    <fileExtension>.png</fileExtension>
  </iconFileType>
  <iconFileType source="../Binaries/TopMenuImage_Shared.png">
    <alias>k:topMenuImage_Shared</alias>
    <name>Shared Menu Image</name>
    <description>
    </description>
    <imageWidth>200</imageWidth>
    <imageHeight>200</imageHeight>
    <fileExtension>.png</fileExtension>
  </iconFileType>
  <iconFileType source="../Binaries/TopMenuImage_Admin.png">
    <alias>k:topMenuImage_Admin</alias>
    <name>Admin Menu Image</name>
    <description>
    </description>
    <imageWidth>200</imageWidth>
    <imageHeight>200</imageHeight>
    <fileExtension>.png</fileExtension>
  </iconFileType>
  <iconFileType source="../Binaries/TopMenuImage_Documents.png">
    <alias>k:topMenuImage_Documents</alias>
    <name>Documents Menu Image</name>
    <description>
    </description>
    <imageWidth>200</imageWidth>
    <imageHeight>200</imageHeight>
    <fileExtension>.png</fileExtension>
  </iconFileType>
  <iconFileType source="../Binaries/TopMenuImage_TestSolution.png">
