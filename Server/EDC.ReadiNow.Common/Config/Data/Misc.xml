    <name>Test Solution Menu Image</name>
    <description>
    </description>
    <imageWidth>200</imageWidth>
    <imageHeight>200</imageHeight>
    <fileExtension>.png</fileExtension>
  </iconFileType>
  <!-- Tenant specific console theme -->
