{
  "esversion": 6,
  // enforcing
  "immed": true,
  "noarg": true,
  "undef": true,
  "bitwise": true,
  "latedef": "nofunc",
  //"maxcomplexity": 15,
  "globals": {
    "console": false,
    "window": false,
    "angular": false,
    "Globalize": false,
    "d3": false,
    "_": false,
    "$": false,
    "jQuery": false,
    "q": false,
    "sp": false,
    "spUtils": false,
    "spEntityUtils": false,
    "spEntity": false,
    "spResource": false,
    "jsonInt": false,
    "jsonString": false,
    "jsonLookup": false,
    "jsonRelationship": false,
    "jsonDate": false,
    "jsonDateTime": false,
    "jsonTime": false,
    "jsonBool": false,
    "jsonDecimal": false,
    "jsonCurrency": false,
    "jsonGuid": false,
    "jasmine": false,
    "describe": false,
    "xdescribe": false,
    "beforeEach": false,
    "afterEach": false,
    "it": false,
    "xit": false,
    "module": false,
    "inject": false,
    "expect": false,
    "spyOn": false,
    "runs": false,
    "waitsFor": false,
    "TestSupport": false
  },
  // relaxing
  "sub": true,
  "eqnull": true,
  "debug": true,
  "smarttabs": true,
  // environments
  "browser": true
}