namespace ReadiMon.Plugin.Database.Status
{
	/// <summary>
	///     Class representing the SqlServerInstance type.
	/// </summary>
	public class SqlServerInstance
	{
		/// <summary>
		///     Initializes a new instance of the <see cref="SqlServerInstance" /> class.
		/// </summary>
		/// <param name="name">The name.</param>
		/// <param name="location">The location.</param>
		public SqlServerInstance( string name, string location )
		{
			Name = name;
			Location = location;
		}

		/// <summary>
		///     Gets or sets the name.
		/// </summary>
		/// <value>
		///     The name.
		/// </value>
		public string Name
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets or sets the location.
		/// </summary>
		/// <value>
		///     The location.
		/// </value>
		public string Location
		{
			get;
			private set;
		}
	}
}