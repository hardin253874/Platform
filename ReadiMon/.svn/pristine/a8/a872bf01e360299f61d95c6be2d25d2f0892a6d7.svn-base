namespace ReadiMon.Plugin.Database.Status
{
	/// <summary>
	///     Class representing the SqlServerService type.
	/// </summary>
	public class SqlServerService
	{
		/// <summary>
		///     Initializes a new instance of the <see cref="SqlServerService" /> class.
		/// </summary>
		/// <param name="name">The name.</param>
		/// <param name="startupType">Type of the startup.</param>
		/// <param name="status">The status.</param>
		/// <param name="lastStartup">The last startup.</param>
		/// <param name="serviceAccount">The service account.</param>
		/// <param name="filename">The filename.</param>
		/// <param name="processId">The process identifier.</param>
		public SqlServerService( string name, string startupType, string status, string lastStartup, string serviceAccount, string filename, int processId )
		{
			Name = name;
			StartupType = startupType;
			Status = status;
			LastStartup = lastStartup;
			ServiceAccount = serviceAccount;
			Filename = filename;
			ProcessId = processId;
		}

		/// <summary>
		///     Gets the name.
		/// </summary>
		/// <value>
		///     The name.
		/// </value>
		public string Name
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the type of the startup.
		/// </summary>
		/// <value>
		///     The type of the startup.
		/// </value>
		public string StartupType
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the status.
		/// </summary>
		/// <value>
		///     The status.
		/// </value>
		public string Status
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the last startup.
		/// </summary>
		/// <value>
		///     The last startup.
		/// </value>
		public string LastStartup
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the service account.
		/// </summary>
		/// <value>
		///     The service account.
		/// </value>
		public string ServiceAccount
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the filename.
		/// </summary>
		/// <value>
		///     The filename.
		/// </value>
		public string Filename
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the process identifier.
		/// </summary>
		/// <value>
		///     The process identifier.
		/// </value>
		public int ProcessId
		{
			get;
			private set;
		}
	}
}