using System;

namespace ReadiMon.Plugin.Database.Status
{
	/// <summary>
	///     Class representing the SqlServerMaintenancePlan type.
	/// </summary>
	public class SqlServerMaintenancePlan
	{
		/// <summary>
		///     Initializes a new instance of the <see cref="SqlServerMaintenancePlan" /> class.
		/// </summary>
		/// <param name="name">The name.</param>
		/// <param name="createdDate">The created date.</param>
		/// <param name="modifiedDate">The modified date.</param>
		/// <param name="enabled">The enabled.</param>
		/// <param name="notifyLevelEmail">The notify level email.</param>
		/// <param name="emailOperator">The email operator.</param>
		public SqlServerMaintenancePlan( string name, DateTime createdDate, DateTime modifiedDate, byte enabled, int notifyLevelEmail, string emailOperator )
		{
			Name = name;
			CreatedDate = createdDate;
			ModifiedDate = modifiedDate;
			Enabled = enabled;
			NotifyLevelEmail = notifyLevelEmail;
			EmailOperator = emailOperator;
		}

		/// <summary>
		///     Gets the name.
		/// </summary>
		/// <value>
		///     The name.
		/// </value>
		public string Name
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the created date.
		/// </summary>
		/// <value>
		///     The created date.
		/// </value>
		public DateTime CreatedDate
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the modified date.
		/// </summary>
		/// <value>
		///     The modified date.
		/// </value>
		public DateTime ModifiedDate
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the enabled.
		/// </summary>
		/// <value>
		///     The enabled.
		/// </value>
		public byte Enabled
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the notify level email.
		/// </summary>
		/// <value>
		///     The notify level email.
		/// </value>
		public int NotifyLevelEmail
		{
			get;
			private set;
		}

		/// <summary>
		///     Gets the email operator.
		/// </summary>
		/// <value>
		///     The email operator.
		/// </value>
		public string EmailOperator
		{
			get;
			private set;
		}
	}
}